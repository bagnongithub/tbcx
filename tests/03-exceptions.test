# -*-Tcl-*-
package require tbcx
package require tcltest 2.5
namespace import ::tcltest::*

test exc.1 {exception ranges: try/catch} -body {
    set in  [makeFile {
        try {
            error FOO
        } on error {msg} {
            return $msg
        }
    } exc.1-in.tcl]
    set out [makeFile "" exc.1-out.tbcx]
    tbcx::save $in $out
    tbcx::load $out
} -result FOO

cleanupTests
